{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": 0,
    "class-methods-use-this": 0,
    "no-plusplus": 2,
    "no-underscore-dangle": "off",
    "no-nested-ternary":"off",
    "camelcase": "warn"
  }
}